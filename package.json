{
  "name": "cli",
  "module": "index.js",
  "version": "0.0.1-alpha",
  "type": "module",
  "scripts": {
    "dev": "parsek",
    "build:windows-x64": "bun build --minify --compile --target=bun-windows-x64 ./index.ts --outfile build/parsek-cli-win-x64.exe",
    "build:linux-x64": "bun build --minify --compile --target=bun-linux-x64 ./index.ts --outfile build/parsek-cli-linux-x64",
    "build:linux-arm64": "bun build --minify --compile --target=bun-linux-arm64 ./index.ts --outfile build/parsek-cli-linux-arm64",
    "build:macos-x64": "bun build --minify --compile --target=bun-darwin-x64 ./index.ts --outfile build/parsek-cli-macos-x64",
    "build:macos-arm64": "bun build --minify --compile --target=bun-darwin-arm64 ./index.ts --outfile build/parsek-cli-macos-arm64",
    "build": "npm-run-all -c build:*",
    "format": "prettier --write --list-different .",
    "postinstall": "alias parsek='bun index.ts'"
  },
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/yargs": "^17.0.33",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
